{
  "name": "myflix",
  "version": "0.1.0",
  "description": "Movie streaming app",
  "main": "main.js",
  "scripts": {
    "start": "electron ./src/main.js",
    "start:dev": "webpack-dev-server",
    "build": "build"
  },
  "keywords": [],
  "build": {
    "appId": "MyFlix",
    "win": {
      "target": "NSIS"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "files": [
      "**/*",
      "build/icon.png"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/TEAMBAMM/myflix.git"
  },
  "author": "TEAMBAMM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TEAMBAMM/myflix/issues"
  },
  "homepage": "https://github.com/TEAMBAMM/myflix#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}
