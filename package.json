{
  "name": "myflix",
  "version": "0.1.0",
  "description": "Movie streaming app",
  "main": "main.js",
  "scripts": {
    "start": "webpack && electron ./src/main.js",
    "build": "build"
  },
  "keywords": [],
  "build": {
    "appId": "MyFlix",
    "win": {
      "target": "NSIS"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "files": [
      "**/*",
      "build/icon.png"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/TEAMBAMM/myflix.git"
  },
  "author": "TEAMBAMM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TEAMBAMM/myflix/issues"
  },
  "homepage": "https://github.com/TEAMBAMM/myflix#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "ajv": "^6.4.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "chokidar": "^2.0.3",
    "evilscan": "^1.6.1",
    "history": "^4.7.2",
    "imdb-api": "^3.1.2",
    "ip": "^1.1.5",
    "libnmap": "^0.4.11",
    "material-ui": "^0.20.0",
    "morgan": "^1.9.0",
    "node-libnmap": "^0.2.15",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-player": "^1.5.0",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "^3.0.2"
  }
}
